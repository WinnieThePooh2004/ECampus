@page "/Students/{GroupId:int}"
@using ECampus.Domain.Requests.Student
@using ECampus.Domain.Responses.Student
<IndexPageModel TData="MultipleStudentResponse" TParameters="StudentParameters"
           ParameterOptions="@(p => { p.GroupId = GroupId; p.OrderBy = "LastName"; })"
           CreateLink="@($"/students/Create/{GroupId}")"
           EditLink="/students/edit"/>

@code{

    [Parameter]
    public int GroupId { get; set; }

}