@page "/submissions/{CourseTaskId:int}"
@using ECampus.Domain.Requests.TaskSubmission
@using ECampus.Domain.Responses.TaskSubmission

<IndexPageModel TData="MultipleTaskSubmissionResponse" TParameters="TaskSubmissionParameters" EditEnabled="false"
                ParameterOptions="parameters => parameters.CourseTaskId = CourseTaskId"
                ActionLinks="@(new List<(string, Func<MultipleTaskSubmissionResponse, string>)> { ("Mark submission", t => $"/markSubmission/{t.Id}") })"/>